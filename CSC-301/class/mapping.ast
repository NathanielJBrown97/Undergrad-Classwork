-- Compute a list whose elements are incremented by oen compared to the input list.

let a = [1,2,3]. --List of integers.
let b = [].  -- empty unneeded list

let b = a @map( lambda with i do i+1).  -- fill list b by mapping through a list incrementing each value.

assert(b == [2, 3, 4]). -- assert the final value of b is equal to the list incremented by 1.